<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="themilks.ajudaeu">

    <application
        android:allowBackup="true"
        android:icon="@drawable/img_1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MenuActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoriasActivity" />
        <activity android:name=".JardimActivity" />
        <activity android:name=".AlvenariaActivity" />
        <activity android:name=".DiaristaActivity" />
        <activity android:name=".InfoActivity" />
        <activity android:name=".EletricaActivity" />
        <activity android:name=".HidraulicaActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>